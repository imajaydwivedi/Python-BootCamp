   "source": [
    "num = int(input(\"Enter number: \"))\n",
    "counter = 1\n",
    "while counter <= 10:\n",
    "    mul = num * counter\n",
    "    print(f\"{num} * {counter} = {mul}\")\n",
    "    counter += 1\n",
    "else:\n",
    "    print(\"***********\")"
   ]
